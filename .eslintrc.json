{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-unused-labels": "warn",
    "no-unused-expressions": "warn",
    "no-duplicate-case": "warn"
  }
}
